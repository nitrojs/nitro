{
  "compilerOptions": {
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": false,
    "allowJs": true,
    "resolveJsonModule": true,
    "paths": {
      "nitropack": [
        "./src/index"
      ],
      "#nitro": [
        "./src/runtime/index"
      ],
      "#nitro/*": [
        "./src/runtime/*"
      ],
      "#nitro-error": [
        "./src/runtime/error"
      ],
    },
  }
}
